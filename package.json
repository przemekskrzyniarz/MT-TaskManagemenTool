{
  "name": "node-typescript-jest-starter",
  "version": "0.0.1",
  "description": "basic node.js, typescript, and jest project starter",
  "main": "frontend/build/index.js",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "boom": "^7.3.0",
    "fastify": "^3.14.0",
    "fastify-gql": "^5.8.2",
    "fastify-swagger": "^4.4.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/boom": "^7.3.0",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongoose": "^5.10.3",
    "@types/node": "^14.14.33",
    "@types/nodemon": "^1.19.0",
    "eslint": "^7.6.0",
    "faker": "^5.4.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^4.2.3",
    "wait-on": "^5.2.1"
  },
  "scripts": {
    "build": "rimraf dist && tsc && cd frontend && npm install && npm run build",
    "buildv2": "rimraf dist && tsc",
    "start": "node dist/index.js",
    "dev": "run-p --race start:app start:frontend",
    "start:app": "cd src && ts-node-dev --respawn --transpileOnly ./index.ts && wait-on tcp:5000",
    "start:frontend": "cd frontend && npm run start",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "seed": "node ./src/helpers/seed.js",
    "lint": "eslint ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
